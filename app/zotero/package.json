{
  "name": "zotero-obsidian-note",
  "version": "0.2.0",
  "description": "Zotero plugin to integrate with Obsidian.md",
  "scripts": {
    "lint": "eslint . --ext .ts --cache --cache-location .eslintcache/",
    "build": "tsc --noEmit && node esbuild.js && zotero-plugin-zipup build zotero-obsidian-note",
    "dev": "python3 start.py",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aidenlx/obsidian-zotero.git"
  },
  "author": {
    "name": "aidenlx",
    "email": "aiden.lx@outlook.com"
  },
  "bugs": {
    "url": "https://github.com/aidenlx/obsidian-zotero/issues"
  },
  "homepage": "https://github.com/aidenlx/obsidian-zotero",
  "dependencies": {
    "lodash-es": "~4.17.21",
    "monkey-around": "^2.3.0",
    "@obzt/common": "workspace:*",
    "@release-it/bumper": "^4.0.0"
  },
  "xpi": {
    "name": "Obsidian Note for Zotero",
    "updateLink": "https://github.com/aidenlx/obsidian-zotero/releases/download/zt{version}/zotero-obsidian-note-{version}.xpi",
    "updateInfoURL": "https://github.com/aidenlx/obsidian-zotero/releases/latest",
    "iconURL": "chrome://zotero-obsidian-note/skin/logo.svg",
    "releaseURL": "https://raw.githubusercontent.com/aidenlx/obsidian-zotero/master/app/zotero/"
  },
  "devDependencies": {
    "@aidenlx/eslint-config": "workspace:*",
    "@types/lodash-es": "~4.17.6",
    "assert-never": "^1.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "@types/mocha": "^9.0.0",
    "esbuild": "^0.17.4",
    "zotero-plugin": "1.4.8",
    "@types/eslint": "~8.4.6",
    "eslint": "~8.31.0",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.9.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}

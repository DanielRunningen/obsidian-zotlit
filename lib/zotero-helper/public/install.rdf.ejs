<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:em="http://www.mozilla.org/2004/em-rdf#">

    <Description about="urn:mozilla:install-manifest">
        <em:id><%= pkg.zotero.id %></em:id>
        <em:name><%= pkg.name %></em:name>
        <em:version><%= pkg.version %></em:version>
        <em:multiprocessCompatible>true</em:multiprocessCompatible>
        <em:updateURL><%= pkg.zotero.update_url %></em:updateURL>
        <em:name><%= pkg.name %></em:name>
        <em:description><%= pkg.description %></em:description>
        <em:creator><%= pkg.author %></em:creator>
        <em:homepageURL><%= pkg.homepage %></em:homepageURL>
        <em:type>2</em:type> <!-- type=extension -->
        <em:iconURL><%= Object.values(pkg.zotero.icons)[0] %></em:iconURL>
        <% if (pkg.zotero.icons["64"]) { %>
        <em:icon64URL><%= pkg.zotero.icons["64"] %></em:icon64URL>
        <% } %>
        <em:bootstrap>true</em:bootstrap>
        <em:targetApplication>
            <Description>
                <em:id>zotero@chnm.gmu.edu</em:id>
                <em:minVersion>6.0</em:minVersion>
                <em:maxVersion>*</em:maxVersion>
            </Description>
        </em:targetApplication>
    </Description>
</RDF>

      "types": "./dist/utils.d.ts"
    }
  },
  "scripts": {
    "build": "rimraf dist && tsc"
  },
  "peerDependencies": {
    "esbuild": "^0.17.4"
  },
  "dependencies": {
    "nanoid": "~4.0.0"
  },
  "devDependencies": {
    "typescript": "^4.9.4",
    "@types/node": "^17.0.10",
    "rimraf": "^3.0.2"
  }
}
